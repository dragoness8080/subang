<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/income_date"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:text="#时间"
        android:paddingLeft="10dp"/>

    <include layout="@layout/layout_splitline" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/comission"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:id="@+id/commission_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="订单佣金"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/commission_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/commission_title"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:text="20.00"
                    android:textColor="@color/orange"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/commission_price"
                    android:layout_alignBaseline="@id/commission_price"
                    android:textSize="14sp"
                    android:textColor="@color/orange"
                    android:text="¥"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/txtGrayColor"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/manager_commission"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:id="@+id/manager_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="订单佣金"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/manager_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/manager_title"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:text="20.00"
                    android:textColor="@color/orange"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/manager_price"
                    android:layout_alignBaseline="@id/manager_price"
                    android:textSize="14sp"
                    android:textColor="@color/orange"
                    android:text="¥"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/txtGrayColor"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/manager_team_commission"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:id="@+id/manager_team_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="订单佣金"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/manager_team_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/manager_team_title"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:text="20.00"
                    android:textColor="@color/orange"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/manager_team_price"
                    android:layout_alignBaseline="@id/manager_team_price"
                    android:textSize="14sp"
                    android:textColor="@color/orange"
                    android:text="¥"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/txtGrayColor"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/stock_commission"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:id="@+id/stock_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="订单佣金"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/stock_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/stock_title"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:text="20.00"
                    android:textColor="@color/orange"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/stock_price"
                    android:layout_alignBaseline="@id/stock_price"
                    android:textSize="14sp"
                    android:textColor="@color/orange"
                    android:text="¥"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/txtGrayColor"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/balance_commission"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:id="@+id/balance_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="订单佣金"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/balance_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/balance_title"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:text="20.00"
                    android:textColor="@color/orange"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/balance_price"
                    android:layout_alignBaseline="@id/balance_price"
                    android:textSize="14sp"
                    android:textColor="@color/orange"
                    android:text="¥"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/txtGrayColor"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/manual_agent"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:id="@+id/manual_agent_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="代理收入"
                    android:layout_marginLeft="10dp"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/manual_agent_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/manual_agent_text"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:text="20.00"
                    android:textColor="@color/orange"
                    android:textSize="20dp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/manual_agent_price"
                    android:layout_alignBaseline="@id/manual_agent_price"
                    android:textSize="14sp"
                    android:textColor="@color/orange"
                    android:text="¥"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/txtGrayColor"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/manual_recommend"
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:id="@+id/manual_recommend_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:text="推荐奖励"
                    android:textSize="16sp"/>

                <TextView
                    android:id="@+id/manual_recommend_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignBaseline="@id/manual_recommend_text"
                    android:layout_marginRight="10dp"
                    android:text="20.00"
                    android:textColor="@color/orange"
                    android:textSize="20sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/manual_recommend_price"
                    android:layout_alignBaseline="@id/manual_recommend_price"
                    android:textSize="14sp"
                    android:textColor="@color/orange"
                    android:text="¥"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/txtGrayColor"/>

            </RelativeLayout>

        </LinearLayout>

    </RelativeLayout>

</LinearLayout>